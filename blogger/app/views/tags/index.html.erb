<h1> All Tags </h1>

<ul id= "Tags">
	<% @tags.each do |tag| %>
		<li>
			<%= link_to tag.name, tag_path(tag),
			class: 'tag-title', id: 'tag_id' %>
			<% if logged_in? %>
				<%= link_to "delete", tag_path(tag), method: :delete,
					data: {confirm: "Really delete the article?"} %>
			<% end %>
		</li>
	<% end %>
</ul>

<%= link_to "<< Back to Articles Index", articles_path %>
